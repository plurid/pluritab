        "@plurid/plurid-icons-react": "^0.1.30",
        "@plurid/plurid-pubsub": "^0.1.10",
        "@plurid/plurid-react": "^0.1.201",
        "@plurid/plurid-themes": "^0.1.26",
        "@plurid/plurid-ui-react": "^0.1.84",
        "apollo-cache-inmemory": "^1.6.6",
        "apollo-client": "^2.6.10",
        "apollo-link-http": "^1.5.17",
        "graphql": "^15.3.0",
        "graphql-tag": "^2.11.0",
        "hammerjs": "^2.0.8",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.1",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "styled-components": "^5.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@babel/register": "^7.10.5",
        "@types/chrome": "^0.0.122",
        "@types/jest": "^26.0.9",
        "@types/node": "^14.0.27",
        "@types/react": "^16.9.44",
        "@types/react-dom": "^16.9.8",
        "@types/styled-components": "^5.1.2",
        "copy-webpack-plugin": "^6.0.3",
        "css-loader": "^4.2.0",
        "html-webpack-plugin": "^4.3.0",
        "jest": "^26.2.2",
        "ts-jest": "^26.1.4",
        "ts-loader": "^8.0.2",
        "typescript": "^3.9.7",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "zip-folder": "^1.0.0"
    }
